{
  "name": "daager/laravel-nemilin",
  "description": "Laravel package for Nemilin API",
  "keywords": [
    "daager",
    "nemilin"
  ],
  "homepage": "https://github.com/daager/laravel-nemilin",
  "license": "LGPL-3.0-only",
  "authors": [
    {
      "name": "Kasymov Nurlan",
      "email": "daager89@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "illuminate/contracts": "^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3"
  },
  "autoload": {
    "psr-4": {
      "Spatie\\Skeleton\\": "src",
      "Spatie\\Skeleton\\Database\\Factories\\": "database/factories"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Spatie\\Skeleton\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit --colors=always",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "DaaGER\\Nemilin\\NemilinerviceProvider"
      ],
      "aliases": {
        "Nemilin": "DaaGER\\Nemilin\\Facade\\Nemilin"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
